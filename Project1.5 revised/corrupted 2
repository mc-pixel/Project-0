label bathroom_intro3:
    if corrupted_bathroom==4:
        jump game_over
    else:
        "This is just a test"
        jump corrupted_bathroom2

label corrupted_bathroom2:
    "Omg it worked"
    $corrupted_bathroom=4
    jump corrupted_bathroom2
